    "/documents/create": {
      "post": {
        "tags": [
          "documents"
        ],
        "summary": "documents/create",
        "description": "Transfers a contract document PDF to be signed by the customer.\n\n Store the returned `document_id` and add it to the identification request for the corresponding user.",
        "operationId": "createDocument",
        "parameters": [
          {
            "in": "path",
            "name": "body",
            "description": "contract document pdf file",
            "required": true,
            "schema": {
              "type": "object",
              "properties": {
                "data": {
                  "type": "string",
                  "format": "binary",
                  "description": "Contract document in PDF format"
                },
                "description": {
                  "type": "string",
                  "description": "Description or title of the contract document"
                }
              }
            }
          }
        ],
        "responses": {
