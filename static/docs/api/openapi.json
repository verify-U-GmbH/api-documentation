                            "example": "P<GBRDOE<<JOHN<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<9138625549GBR4503093M1601013<<<<<<<<<<<<<<00"
                          },
                          "number": {
                            "type": "string",
                            "nullable": true,
                            "description": "The number of this ID document",
                            "example": "C1544JKK40"
                          }
                        }
                      },
                      "id_document_secondary": {
                        "type": "object",
                        "nullable": true,
                        "description": "Support document to the primary ID document",
                        "properties": {
                          "country": {
                            "type": "string",
                            "nullable": true,
                            "description": "Country which issued this ID document",
                            "example": "UK"
                          },
                          "document_type": {
                            "type": "string",
                            "nullable": true,
                            "description": "Type of this ID document",
                            "example": "ID_CARD"
                          },
                          "expiration_date": {
                            "type": "string",
                            "nullable": true,
                            "format": "date",
                            "description": "Date at which this ID document expires",
                            "example": "30.05.2028"
                          },
                          "id": {
                            "type": "string",
                            "nullable": true,
                            "format": "uuid",
                            "description": "ID of this ID document",
                            "example": "c4d49f29-d351-416b-b261-6b42c46081fb"
                          },
                          "issue_date": {
                            "type": "string",
                            "nullable": true,
                            "format": "date",
                            "description": "Date at which this ID document was issued",
                            "example": "30.05.2018"
                          },
                          "mrz": {
                            "type": "string",
                            "nullable": true,
                            "description": "The 'machine-readable zone' entry of this ID document",
                            "example": "P<GBRDOE<<JOHN<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<9138625549GBR4503093M1601013<<<<<<<<<<<<<<00"
                          },
                          "number": {
                            "type": "string",
                            "nullable": true,
                            "description": "The number of this ID document",
                            "example": "C1544JKK40"
                          }
                        }
                      },
                      "last_name": {
                        "type": "string",
                        "nullable": true,
                        "description": "The client's last name",
                        "example": "Doe"
                      },
                      "primary_id_document_number": {
                        "type": "string",
                        "nullable": true,
                        "description": "Number of the primary ID document",
                        "example": "ABC123456"
                      },
                      "rejected": {
                        "type": "boolean",
                        "description": "Indicates whether identification has been rejected"
                      },
                      "rejection_cause": {
                        "type": "string",
                        "description": "Identification rejection cause",
                        "nullable": true,
                        "example": "DOCUMENT_MISSING"
                      },
                      "state": {
                        "type": "string",
                        "description": "OAuth parameter in format `[CLIENT_REFERENCE_ID]:[SESSION_ID]` supplied to identification gateway",
                        "example": "demo_client@demo.com:3a837788-6d72-40ce-818c-d92cc6c9b3d8"
                      }
                    }
                  }
                }
              }
            }
          },
          "422": {
            "description": "Client side error",
            "content": {
              "application/json": {
                "schema": {
                  "type": "object",
                  "additionalProperties": {
                    "type": "string"
                  },
                  "example": {
                    "identification_id": "not found"
